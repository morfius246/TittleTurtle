@model TitleTurtle.Models.EditUser
@{
    ViewBag.Title = "Profile";
}
@using (Html.BeginForm("EditUser", "Account", FormMethod.Post)) {
   

<fieldset>
        
        <p>@ViewBag.Message</p>

        @Html.HiddenFor(model => model.UserID)
        @Html.HiddenFor(model => model.Login)
        <div class="editor-label">
            @Html.LabelFor(model => model.UserFirstName)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.UserFirstName)
            
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.UserLastName)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.UserLastName)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ContactEmail)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ContactEmail)
     
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ContactMobile)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ContactMobile)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.PersDataDate)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.PersDataDate)
        </div>
    <a class="navli-user" href="/Account/Manage" title="Manage">Изменить пароль</a>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}



